# Da01_SQL
EX1: SELECT NAME FROM CITY WHERE COUNTRYCODE = 'USA' AND POPULATION > 120000;
EX2: SELECT * FROM CITY WHERE CountryCode = 'JPN'
EX3: SELECT CITY, STATE FROM STATION;
EX4: SELECT CITY FROM STATION WHERE LOWER(SUBSTR(CITY,1,1)) IN ('a','e','i','o','u');
EX5: SELECT DISTINCT(CITY) FROM STATION WHERE CITY LIKE '%a' OR CITY LIKE '%e' OR CITY LIKE '%i' OR CITY LIKE '%o' 
OR CITY LIKE '%u';
EX6: SELECT DISTINCT CITY FROM STATION WHERE UPPER(SUBSTR(CITY,1,1)) NOT IN ('A','E','I','O','U') AND LOWER(SUBSTR(CITY,1,1)) NOT IN
('a','e','i','o','u'); 
EX7: SELECT name FROM Employee ORDER BY name ASC;
EX8: SELECT name FROM Employee WHERE salary > 2000 AND months < 10 ORDER BY employee_id; 
EX9: SELECT product_iD FROM Products WHERE low_fats = 'Y' AND recyclable = 'Y';
EX10
EX11: SELECT DISTINCT name, population, area FROM World WHERE area >=3000000 OR population >= 25000000;
Ex12: SELECT DISTINCT author_id AS id from Views WHERE author_id = viewer_id ORDER BY author_id ASC;
EX13: SELECT name FROM customer WHERE referee_id <> 2 OR referee_id IS NULL;
EX14: SELECT part,assembly_step FROM parts_assembly WHERE finish_date IS NULL;
EX15: select * from lyft_drivers where yearly_salary <= 30000 or yearly_salary >70000;
EX16: select * from uber_advertising WHERE year = 2019 and money_spent > 100000;
